<%=  form_tag action: "index", html: { multipart: true, role:"form", class:"form-horizontal"} do %>

  <div class="form-group">
    <label class="col-sm-2 control-label"> Registration Id</label>
    <%= text_field_tag :registration_id %>
  </div>

  <div class="form-group">
    <label class="col-sm-offset-2"> Or</label>
  </div>


  <div class="form-group">
    <label class="col-sm-2 control-label"> Upload child's photo</label>
    <%= file_field_tag :photo %>
  </div>

  <div class="form-group">
    <div class="col-sm-offset-2">
      <%= submit_tag :submit %>
    </div>
  </div>
 <% end %>


